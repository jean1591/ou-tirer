{
  "name": "release-fast",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=development bun x expo start -c --go",
    "start:prod": "NODE_ENV=production bun x expo start -c",
    "android": "NODE_ENV=development bun x expo start --android",
    "ios": "NODE_ENV=development bun x expo start --ios",
    "web": "NODE_ENV=development bun x expo start --web",
    "test": "jest --watchAll",
    "build:dev": "NODE_ENV=development bun x eas-cli build --profile dev",
    "build:dev:sim": "NODE_ENV=development bun x eas-cli build --profile dev-sim --platform ios",
    "build:preview": "NODE_ENV=test bun x eas-cli build --profile preview",
    "build:prod": "NODE_ENV=production bun x eas-cli build --profile production --auto-submit",
    "build:prod:local": "NODE_ENV=production bun x eas-cli build --profile production --local --non-interactive",
    "submit": "npx eas-cli submit",
    "translate": "npm run extract && node ./scripts/mergeTranslations.ts",
    "extract": "formatjs extract './{app,src}/**/*.ts*' --ignore='**/*.d.ts' --out-file ./assets/translations/en.json --format simple"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "14.0.4",
    "@formatjs/cli": "6.3.15",
    "@gorhom/bottom-sheet": "5.0.6",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/hooks": "3.0.0",
    "@react-navigation/native": "7.0.14",
    "@rn-primitives/portal": "^1.1.0",
    "@rn-primitives/slot": "^1.1.0",
    "@rn-primitives/switch": "^1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "@sentry/react-native": "6.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "4.1.0",
    "expo": "52.0.23",
    "expo-blur": "14.0.1",
    "expo-build-properties": "0.13.1",
    "expo-constants": "17.0.3",
    "expo-dev-client": "5.0.8",
    "expo-font": "13.0.2",
    "expo-linear-gradient": "14.0.1",
    "expo-linking": "7.0.3",
    "expo-localization": "16.0.0",
    "expo-navigation-bar": "~4.0.6",
    "expo-notifications": "0.29.11",
    "expo-router": "4.0.15",
    "expo-sharing": "13.0.0",
    "expo-splash-screen": "0.29.18",
    "expo-status-bar": "2.0.0",
    "expo-store-review": "8.0.0",
    "expo-system-ui": "4.0.6",
    "expo-tracking-transparency": "5.1.0",
    "expo-web-browser": "14.0.1",
    "lucide-react-native": "^0.469.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-intl": "7.1.4",
    "react-native": "0.76.6",
    "react-native-country-flag": "2.0.2",
    "react-native-gesture-handler": "2.20.2",
    "react-native-google-mobile-ads": "14.8.0",
    "react-native-purchases": "8.5.0",
    "react-native-reanimated": "3.16.6",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "0.19.13",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@types/react": "18.3.12",
    "jest": "29.7.0",
    "jest-expo": "52.0.3",
    "react-test-renderer": "18.3.1",
    "typescript": "5.7.2"
  },
  "private": true
